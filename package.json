{
  "name": "practicets",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start-server": "node dist/server.js",
    "start-dev-server": "tsc-watch --onSuccess \"node dist/server.js \"",
    "dev": "npm-run-all clean build start-dev-server",
    "run-populate-db-script": "node dist/database/populate-db.js",
    "populate-db": "npm-run-all clean build run-populate-db-script",
    "run-delete-db-script": "node dist/database/delete-db.js",
    "delete-db": "npm-run-all clean build run-delete-db-script",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "md-to-quip": "npx ts-node scripts/md-to-quip.ts"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "body-parser": "^2.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.10",
    "typeorm": "^0.3.28",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.3.3"
  }
}
